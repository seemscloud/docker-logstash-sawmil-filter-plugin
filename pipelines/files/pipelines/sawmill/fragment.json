{
  "steps": [
    {
      "rename": {
        "config": {
          "from": "message",
          "to": "@message"
        }
      }
    },
    {
      "date": {
        "config": {
          "field": "foo",
          "targetField": "datetime",
          "formats": [
            "ISO8601"
          ]
        },
        "onSuccess": [
          {
            "addTag": {
              "config": {
                "tags": [
                  "success/kvx"
                ]
              }
            }
          }
        ],
        "onFailure": [
          {
            "addTag": {
              "config": {
                "tags": [
                  "fail/kvx"
                ]
              }
            }
          }
        ]
      }
    }
  ]
}